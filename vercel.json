{
  "routes": [
    {
      "src": "/api/chat",
      "methods": ["OPTIONS", "POST"],
      "dest": "/api/chat.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://drholi.webflow.io",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      },
      "continue": true
    },
    { "handle": "filesystem" }
  ]
}
